{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "workspaces": {
        "packages/fnpm": {
            "entry": ["src/{fnpm,fnpx}.ts!"]
        },
        "packages/fnpm-doctor": {
            "entry": [
                "src/index.ts!",
                "src/scanner/index.ts!",
                "src/scanner/rule.ts!"
            ]
        },
        "packages/fnpm-toolkit": {
            "entry": ["src/index.ts!"]
        },
        "packages/fnpm-ui": {
            "entry": [
                "app/root.tsx!",
                "app/entry.{client,server}.{ts,tsx}!",
                "app/routes/**/*.{ts,tsx}!",
                "**/*.config.{ts}!"
            ]
        },
        "packages/pm-combo": {
            "entry": ["src/index.ts!"]
        },
        "packages/npm-registry-client": {
            "entry": [
                "src/index.ts!",
                "src/api.ts!",
                "src/client.ts!",
                "src/registry.ts!"
            ]
        }
    },
    "ignore": ["**/use-mobile.tsx", "**/*.generated.ts", "**/ui/**"],
    "ignoreDependencies": ["tslib", "tailwindcss-animate"]
}
