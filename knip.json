{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "index.{js,ts,tsx}",
        "src/index.{js,ts,tsx}",
        "*.config.{js,ts,tsx}"
    ],
    "workspaces": {
        "packages/fnpm": {
            "entry": ["src/{fnpm,fnpx}.ts!"]
        },
        "packages/fnpm-doctor": {
            "entry": [
                "src/index.ts!",
                "src/scanner/index.ts!",
                "src/scanner/rule.ts!"
            ]
        },
        "packages/fnpm-toolkit": {
            "entry": ["src/index.ts!"]
        },
        "packages/fnpm-ui": {
            "entry": [
                "app/root.tsx!",
                "app/entry.{client,server}.{ts,tsx}!",
                "app/routes/**/*.{ts,tsx}!"
            ]
        },
        "packages/pm-combo": {
            "entry": ["src/index.ts!"]
        },
        "packages/npm-registry-client": {
            "entry": ["src/*.ts!"]
        }
    },
    "ignore": ["**/use-mobile.tsx", "**/*.generated.ts", "**/ui/**"],
    "ignoreDependencies": ["tslib", "tailwindcss-animate"]
}
