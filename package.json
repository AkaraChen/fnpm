{
    "name": "fnpm-workspace",
    "private": true,
    "version": "1.12.2",
    "description": "",
    "scripts": {
        "check": "biome check . --write",
        "build": "turbo build",
        "dev": "turbo dev",
        "test": "turbo test",
        "release": "bumpp -r && pnpm -r publish",
        "knip": "knip --strict --exclude unlisted",
        "circular": "madge --circular --extensions ts ./packages",
        "syncpack": "syncpack",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.11.1",
    "devDependencies": {
        "@akrc/tsconfig": "^2.0.0",
        "@biomejs/biome": "^1.9.4",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "^22.15.30",
        "bumpp": "^10.1.1",
        "husky": "9.1.7",
        "knip": "^5.60.2",
        "lint-staged": "^16.1.0",
        "madge": "^8.0.0",
        "syncpack": "13.0.4",
        "tslib": "^2.8.1",
        "tsup": "^8.5.0",
        "turbo": "^2.5.4",
        "typescript": "^5.8.3",
        "vitest": "^3.2.2"
    },
    "pnpm": {
        "patchedDependencies": {
            "parse-package-name@1.0.0": "patches/parse-package-name@1.0.0.patch"
        },
        "overrides": {
            "array-flatten": "npm:@nolyfill/array-flatten@^1.0.44",
            "is-arguments": "npm:@nolyfill/is-arguments@^1.0.44",
            "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
            "is-generator-function": "npm:@nolyfill/is-generator-function@^1.0.44",
            "is-typed-array": "npm:@nolyfill/is-typed-array@^1.0.44",
            "isarray": "npm:@nolyfill/isarray@^1.0.44",
            "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44",
            "safer-buffer": "npm:@nolyfill/safer-buffer@^1.0.44",
            "side-channel": "npm:@nolyfill/side-channel@^1.0.44",
            "which-typed-array": "npm:@nolyfill/which-typed-array@^1.0.44"
        }
    },
    "lint-staged": {
        "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
            "biome check --write --no-errors-on-unmatched"
        ]
    }
}
