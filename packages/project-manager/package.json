{
    "name": "@akrc/project-manager",
    "version": "0.0.1",
    "description": "Project entity management",
    "main": "./dist/es/index.js",
    "scripts": {
        "build": "bunchee --minify",
        "dev": "bunchee --watch",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "lint": "eslint ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.15.0",
    "type": "module",
    "dependencies": {
        "better-sqlite3": "^12.2.0",
        "drizzle-orm": "^0.44.5",
        "drizzle-zod": "^0.8.3",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "bunchee": "6.5.2",
        "drizzle-kit": "^0.31.4"
    },
    "files": [
        "dist"
    ],
    "module": "./dist/es/index.js",
    "types": "./dist/es/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/es/index.d.ts",
                "default": "./dist/es/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.cts",
                "default": "./dist/cjs/index.cjs"
            }
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
