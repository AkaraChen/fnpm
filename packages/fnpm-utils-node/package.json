{
    "name": "fnpm-utils-node",
    "version": "1.13.1",
    "description": "",
    "scripts": {
        "dev": "tsdown --watch",
        "build": "tsdown",
        "lint": "eslint ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.17.1",
    "type": "module",
    "devDependencies": {
        "@pnpm/types": "1000.9.0",
        "package-directory": "^8.1.0",
        "read-pkg": "^9.0.1",
        "tsdown": "^0.14.2"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@akrc/monorepo-tools": "^4.1.0",
        "effect": "^3.17.9"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./mt": {
            "import": {
                "types": "./dist/mt.d.ts",
                "default": "./dist/mt.js"
            },
            "require": {
                "types": "./dist/mt.d.cts",
                "default": "./dist/mt.cjs"
            }
        },
        "./pkg": {
            "import": {
                "types": "./dist/pkg.d.ts",
                "default": "./dist/pkg.js"
            },
            "require": {
                "types": "./dist/pkg.d.cts",
                "default": "./dist/pkg.cjs"
            }
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
